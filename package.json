{
    "name": "mbmi-canjs",
    "version": "0.0.1",
    "description": "",
    "main": "index.js",
    "dependencies": {
        "bootstrap": "3.3.7",
        "jquery": "3.2.1",
        "steal": "^1.5.8",
        "can-component": "3.3.3",
        "can-connect": "1.5.8",
        "can-route": "3.2.1",
        "can-stache": "3.6.1",
        "can-map": "3.3.3",
        "can-map-define": "3.0.9"
    },
    "devDependencies": {
        "can-fixture": "1.1.0",
        "steal-tools": "^1.8.2",
        "steal-css": "1.2.5",
        "steal-less": "1.2.0",
        "steal-stache": "^3.1.1"
    },
    "steal": {
        "bundle": [
            "components/authorize",
            "components/menu"
        ],
        "meta": {
            "models/fixtures": {
                "bundle": false
            },
            "bootstrap": {
                "deps": [
                    "bootstrap/dist/css/bootstrap.css"
                ]
            }
        },
        "plugins": [
            "steal-css",
            "steal-less",
            "steal-stache"
        ]
    },
    "scripts": {
        "clear": "node -e 'require(\"./scripts/build.js\").clear(\"build\")'",
        "build": "npm run clear && node  -e 'var a = require(\"./scripts/build.js\"); a.build()'",
        "server": "node  -e 'var a = require(\"./scripts/utils.js\"); a.runServer(\"build/bundles\")'",
        "export": "npm run clear && node -e 'require(\"./scripts/build.js\").export()'",
        "sass": "node-sass --version"
    }
}
